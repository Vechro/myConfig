{
    "extends": "stylelint-config-standard",
    "rules": {
        "indentation": 4,
        "number-leading-zero": "always",
        "unit-blacklist": [
            "cm",
            "mm",
            "q",
            "in",
            "pc",
            "pt",
            "px"
        ],
        "selector-list-comma-newline-after": "never-multi-line",
        "no-missing-end-of-source-newline": null
    }
}